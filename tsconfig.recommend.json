{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Libertygame",
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "ESNext",
            "DOM"
        ]
    },
    "include": [
        "node_modules/libertygame-types"
    ]
}